{
    "name": "craftcodery/clicksend",
    "description": "ClickSend integration for Laravel",
    "keywords": [
        "clicksend"
    ],
    "homepage": "https://github.com/craftcodery/clicksend",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "barryvdh/laravel-dompdf": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/http": "^9.0",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "CraftCodery\\ClickSend\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CraftCodery\\ClickSend\\ClickSendServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
